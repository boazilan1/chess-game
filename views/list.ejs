  <form action="/" method="post" id="myForm">
    <table>
      <% Board.forEach(row=>{ %>
        <tr>
          <% row.forEach(Piece=>{ %>
            <% if (Piece!=null) { %>

              <td id="<%= Piece.position%>" class="<%= cls %> <%= Piece.tipey %> <%= Piece.color %> <%= Piece.move %>">
              </td>
              <% } %>

                <% if (cls=="wait_box" ) {cls="black_box" }else{cls="wait_box" } %>

                  <% }); %>
                    <% if (cls=="wait_box" ) {cls="black_box" }else{cls="wait_box" } %>
        <tr>
          <% }); %>
    </table>
  <table>
