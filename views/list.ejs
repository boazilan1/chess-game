      <td class="sideBox">
        <h1 id="chess" class="sideBoxText"></h1>
      </td>
      <td class="sideBox">
        <h1 id="turn" class="<%=turn %> sideBoxText">
          <%=turn %>
      </td>
    </tr>
  </table>
